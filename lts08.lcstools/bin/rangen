#
if [[ $# != 3 || ${1} == "-?" ]]
then
	echo "usage: ${0} minvalue maxvalue howmany"
	exit 0
fi
#
integer minvalue
integer maxvalue
integer howmany
#
minvalue=${1}
maxvalue=${2}
howmany=${3}
#
integer range
integer r
integer rdiv
integer rrem
integer imax
integer i
#
imax=${howmany:-100}
range=$((maxvalue-minvalue+1))
#
i=1
#
while [[ $i -le $imax ]]
do
	r=$RANDOM
	rdiv=$((r/range))
	rrem=$((minvalue+r-rdiv*range))
	#
	echo "Number $i: $rrem"
	i=$((i+1))
done
#
exit 0
